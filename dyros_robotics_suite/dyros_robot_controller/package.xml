<package format="3">
  <name>dyros_robot_controller</name>
  <version>0.2.0</version>
  <description>Robot controller with C++ and Python interface.</description>
  <maintainer email="yoonjh98@snu.ac.kr">Yoon Junheon</maintainer>
  <license>Apache License 2.0</license>

  <!-- Buildtool -->
  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <!-- Dependencies -->
  <depend>rclcpp</depend>
  <depend>rclpy</depend>
  <depend>Eigen3</depend>
  <depend>pinocchio</depend>
  <depend>mujoco_ros_sim</depend>
  <depend>std_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>

  <export>
    <!-- <build_type>ament_python</build_type> -->
    <build_type>ament_cmake</build_type>
    <!-- <build_type>ament_cmake_python</build_type> -->
  </export>
</package>
