<?xml version="1.0" encoding="utf-8"?>
<robot name="husky_robot">
  
  <!-- Planar base -->
  <link name="world"/>
  <link name="x_slider"/>
  <link name="y_slider"/>
  <link name="pivot"/>

  <joint name="x_base_joint" type="prismatic">
      <parent link="world"/>
      <child link="x_slider"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <axis xyz="1 0 0"/>
      <limit effort="1000.0" lower="-10" upper="10" velocity="1.0"/>
  </joint>

  <joint name="y_base_joint" type="prismatic">
      <parent link="x_slider"/>
      <child link="y_slider"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <axis xyz="0 1 0"/>
      <limit effort="1000.0" lower="-10" upper="10" velocity="1.0"/>
  </joint>

  <joint name="pivot_joint" type="revolute">
      <parent link="y_slider"/>
      <child link="pivot"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <axis xyz="0 0 1"/>
      <limit effort="1000.0" velocity="1.0" upper="10.0" lower="-10.0"/>
  </joint>

  <!-- Mobile Base -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://husky_description/meshes/base_link.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.061875"/>
      <geometry>
        <box size="0.9874 0.5709 0.12375"/>
      </geometry>
    </collision>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.17562499999999998"/>
      <geometry>
        <box size="0.7899200000000001 0.5709 0.10375"/>
      </geometry>
    </collision>
  </link>

  <!-- Base footprint is on the ground under the robot -->
  <link name="base_footprint"/>
  <joint name="base_footprint_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 -0.13228"/>
    <parent link="base_link"/>
    <child link="base_footprint"/>
  </joint>

  <joint name="diff_base_flange" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.13228"/>
      <parent link="pivot"/>
      <child link="base_link"/>
  </joint>

